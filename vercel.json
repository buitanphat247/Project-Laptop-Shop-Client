{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/build/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/build/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/build/logo512.png"
    },
    {
      "src": "/robots.txt",
      "dest": "/build/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/build/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://project-laptop-shop-sever.vercel.app/api/v1",
    "REACT_APP_ENV": "production"
  }
}
